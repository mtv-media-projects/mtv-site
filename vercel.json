{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/", 
        "dest": "/main.html"
      },
      {
        "src": "/.*",
        "dest": "/$1.html"
      },
      {
        "src": "/.*",
        "status": 404,
        "dest": "/404.html"
      }
    ]
  }