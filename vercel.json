{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/main.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    },
    {
      "src": "/404",
      "dest": "/404.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404"
    }
  ]
}
