{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/main.html"
    },
    {
      "src": "/main",
      "dest": "/main.html"
    },
    {
      "src": "/cringe-lounge",
      "dest": "/cringe-lounge.html"
    },
    {
      "src": "/new-site",
      "dest": "/new-site.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/test",
      "dest": "/test.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
    
  ]  
}
